<%= form_for(project) do |f| %>
  <div><%= f.label :name %>
  <%= f.text_field :name %></div>
  <div><%= f.label :company %>
  <%= f.text_field :company %></div>
  <div><%= f.label :description %>
  <%= f.text_area :description %></div>
  <div><%= f.label :url_path %>
  <%= f.text_field :url_path %></div>
  <%= f.submit %>
<% end %>